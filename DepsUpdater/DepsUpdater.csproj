<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <Version>1.0.10</Version>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>depsupdater</ToolCommandName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Meziantou.Framework" Version="3.0.23" />
    <PackageReference Include="Meziantou.Framework.DependencyScanning" Version="1.0.0-alpha.7" />
    <PackageReference Include="Meziantou.Framework.Globbing" Version="1.0.11" />
    <PackageReference Include="NuGet.Protocol" Version="6.0.0" />
    <PackageReference Include="System.CommandLine" Version="2.0.0-beta1.21216.1" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="Microsoft.SourceLink.GitHub" Version="1.1.1" />
  </ItemGroup>

</Project>
